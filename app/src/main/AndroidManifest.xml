<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cr.cartago.paraiso.cachi.loaiza.developpersoftware.managementgym">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.Pesas"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".activities.AddCustomer"
            android:parentActivityName=".activities.AddCustomersToday" />
        <activity
            android:name=".activities.AddCustomersToday"
            android:parentActivityName=".activities.Pesas" />
        <activity
            android:name=".activities.AllCustomers"
            android:launchMode="singleTop"
            android:parentActivityName=".activities.Pesas"></activity>
        <activity android:name=".activities.AddPayment"></activity>
    </application>

</manifest>